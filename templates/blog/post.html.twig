{% extends 'base.html.twig' %}

{% block title %}{{ post.title }}{% endblock %}

{% block body %}
    <p>ID {{ id }}</p>
    <h3>{{ post.title|upper}}</h3>
    <p>{{ post.text }}</p>
    {% if post.date is defined %}
        <p>{{ post.date|date('H:i:s') }}</p>
    {% endif %}
    <p>{{ 5600000|price }}</p>
{% endblock %}